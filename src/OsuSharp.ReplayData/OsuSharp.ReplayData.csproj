	<ItemGroup>
		<PackageReference Include="SevenZip" Version="19.0.0" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\OsuSharp.Domain\OsuSharp.Domain.csproj" />
	</ItemGroup>
</Project>