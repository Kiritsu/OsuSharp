<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <VersionPrefix>6.4.1</VersionPrefix>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>disable</Nullable>
    <Authors>Allan Mercou</Authors>
    <Description>An osu!api v1 wrapper</Description>
    <PackageProjectUrl>https://github.com/Kiritsu/OsuSharp</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/Kiritsu/OsuSharp/blob/feature/api-v2/LICENSE.txt</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/Kiritsu/OsuSharp</RepositoryUrl>
    <PackageTags>osu osusharp osu!api api csharp dotnet</PackageTags>
    <RepositoryType>Git</RepositoryType>
    <LangVersion>default</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="SevenZip" Version="19.0.0" />
  </ItemGroup>

</Project>